class Person:
    def __init__(self, name, age):
        self.name = name
        self.age = age

    def get_age(self):
        return self.age

class Driver(Person):
    def __init__(self, name, age, license_number):
        super().__init__(name, age)
        self.license_number = license_number

driver = Driver("Іван", 30, "AB123456")
print(driver.name, driver.get_age(), driver.license_number)  # Іван 30 AB123456
